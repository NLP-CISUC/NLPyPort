#!/home/jd/Dropbox/Agente/2oSem/envir/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'exrex==0.10.5','console_scripts','exrex'
__requires__ = 'exrex==0.10.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('exrex==0.10.5', 'console_scripts', 'exrex')()
    )
